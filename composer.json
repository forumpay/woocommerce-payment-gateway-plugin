    "forumpay/payment-gateway-php-client": "^1.1.3",
    "psr/log":">=1.1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.0",
    "php-stubs/wordpress-stubs": "^5.0@stable",
